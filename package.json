{
  "name": "agricultural-dashboard",
  "version": "1.0.0",
  "description": "Modern React dashboard for agricultural monitoring and management",
  "author": "Emilio",
  "license": "MIT",
  "private": false,
  "keywords": [
    "react",
    "dashboard",
    "agriculture",
    "monitoring",
    "visualization",
    "iot",
    "sensors"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dashboard/issues"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "server": "node backend/server.js",
    "server:dev": "nodemon backend/server.js"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.13.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^8.38.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^2.8.7"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
